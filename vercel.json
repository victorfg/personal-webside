{
  "version": 2,
  "builds": [
    {
      "src": "next.config.mjs",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      },
      "env": {
        "NODE_OPTIONS": "--enable-source-maps"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
